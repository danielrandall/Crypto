#!/usr/bin/gnuplot
 
set terminal postscript
set output '| ps2pdf - solutionDownloadTimes.pdf'

set title "Solution download times"
set xlabel "The size of the file downloaded (MB)"
set ylabel "Average time taken to download (seconds)"
set ytics "50"
plot "data" title "" with lines lt rgb "red"
pause -1 "Hit any key to continue"
